<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="test.com.git.contact">
	<select id="CONTATC_SELECT_ALL" resultType="test.com.git.contact.model.ContactVO">
		select * from contact
	</select>
	
	<select id="CONTACT_SELECT_ONE" resultType="test.com.git.contact.model.ContactVO">
		select * from contact where id = #{id}
	</select>
	
	<insert id="CONTACT_INSERT" parameterType="test.com.git.contact.model.ContactVO">
		insert into contact (id, title, content, con_date, attach_img, user_id, seller_id, act_id, act_name)
		values (#{id}, #{title}, #{content}, #{con_date}, #{attach_img}, #{user_id}, #{seller_id}, #{act_id}, #{act_name})
	</insert>
	
	<delete id="CONTACT_DELETE">
		delete from contact where id = #{id}
	</delete>
</mapper>
